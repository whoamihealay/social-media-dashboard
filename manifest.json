{
    "manifest_version": 2,
    "short_name": "SocialMediaDashboard",
    "name": "Frontend Mentor | Social Media Dashboard",
    "start_url": "/",
    "display": "standalone",
    "theme_color": "#000000",
    "background_color": "#ffffff",
    "icons": [
        {
            "src": "/images/favicon.ico",
            "sizes": "64x64 32x32 24x24 16x16" ,
            "type":"image/x-icon",
            "purpose": "any maskable"
        },{
            "src": "/images/icon-down.svg",
            "sizes": "any" ,
            "type":"image/svg+xml"
        },{
            "src": "/images/icon-up.svg",
            "sizes": "any" ,
            "type":"image/svg+xml"
        },{
            "src": "/images/icon-facebook.svg",
            "sizes": "any" ,
            "type":"image/svg+xml"
        },{
            "src": "/images/icon-youtube.svg",
            "sizes": "any" ,
            "type":"image/svg+xml"
        },{
            "src": "/images/icon-twitter.svg",
            "sizes": "any" ,
            "type":"image/svg+xml"
        },{
            "src": "/images/icon-instagram.svg",
            "sizes": "any" ,
            "type":"image/svg+xml"
        }
    ]

}